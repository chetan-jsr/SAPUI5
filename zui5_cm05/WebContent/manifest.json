{
	"_version": "1.3.0",
	"sap.app": {
		"id": "sapui5.app05",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}"
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sapui5.app05.view.zjs_view_container",
			"type": "JS"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sapui5.app05.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"viewType": "JS",
				"viewPath": "sapui5.app05.view",
				"targetControl":"appContainer",
				"targetAggregation": "pages",
				"routerClass": "sap.m.routing.Router"
			},
			"routes": [{
				"pattern": "",
				"name": "firstView",
				"view":"zjs_view_05_01",
				"targetAggregation": "pages"
			},
			{
				"pattern": "second",
				"name": "secondView",
				"view":"zjs_view_05_02",
				"targetAggregation": "pages"
			}
			]
		}
	}
}